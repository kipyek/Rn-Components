{
  "expo": {
    "name": "Rn-Components",
    "slug": "Rn-Components",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "owner": "ngamia-haulers-platform-limited",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splashs.png",
      "resizeMode": "contain",
      "backgroundColor": "#22275C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.denis504.Rn-Components"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}